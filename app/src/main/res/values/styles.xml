<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>


    </style>
    <style name="BottomNavThem" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="itemTextColor">@color/bottom_nav_text_color</item>

        <item name="colorPrimary">@color/bottom_nav_selected_item</item>

        <item name="backgroundTint">@color/bottom_nav_item_tint</item>
     <!--   <item name="itemIconTint">@color/bottom_nav_background</item>
        <item  name="itemBackground">@color/bottom_nav_background</item>-->

    </style>

    <style name="ThemeOverlay.SearchView" parent="">
        <!-- Text color -->
        <item name="android:editTextColor">@color/text_color</item>
        <!-- Hint text color -->
        <item name="android:textColorHint">@color/text_color_nrml</item>
            <item name="android:background">@drawable/rounded_corners</item>
        <item name="closeIconTint">@color/bottom_nav_item_tint</item>


    </style>

</resources>